import java.io.*;
import java.util.*;
class CircleArea3
{
double x,y,r;
double area()
{
return 22/7*r*r;
}
}
void SetCircle(double a,double b,double c) //parameters
{
x=a;
y=b;
r=c;
public static void main(String args[])
{
Scanner sc= new Scanner(System.in);
